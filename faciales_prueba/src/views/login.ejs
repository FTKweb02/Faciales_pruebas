  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Login</title>

  <link rel="icon" href="/img/Logo-FTK-new-negro.png">
  <link rel="stylesheet" href="/css/style.css">

</head>

<body>
  <div>
    <nav class="color-nav">
      <a href="/">
        <input type="image" id="image" alt="Home" src="/img/Logo-FTK-new-blanco.png" width="150px" />
      </a>
    </nav>
  </div>
  <br />
  <div align="center">
    <div>
      <h1>Inicio de Sesión</h1>
      <form action="/ccenter">
        <br />
        <input type="text" placeholder="ID" name="id">
        <br />
        <br />
        <input id="password-field" type="password" name="password" placeholder="Contraseña" minlength="8"
          maxlength="12">
        <br />
        <br />
        <a href="/ccenter">
          <button class="buttong" type="submit" name="is">Iniciar sesión</button>
        </a>
      </form>
      <br>
    </div>
    <div class="botomnes">
      <a href="/contactc">
        <button>Contact Center</button>
      </a>
      <a href="/ccenter">
        <button>Vendedor</button>
      </a>
      <a href="/facialista">
        <button>Facialistas</button>
      </a>
      <a href="/config">
        <button>Administrador</button>
      </a>
    </div>
  </div>

</body>

</html>